{
  "name": "@nik-kita/brickyard",
  "version": "0.4.1",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "compilerOptions": {
    "noUnusedLocals": false,
    "strictNullChecks": true,
    "strictBindCallApply": true
  },
  "exclude": [
    "dist",
    "node_modules",
    ".vite"
  ],
  "lint": {
    "rules": {
      "exclude": [
        "no-explicit-any",
        "ban-types",
        "no-empty",
        "no-unused-vars",
        "require-await"
      ]
    }
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.42.1",
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@std/fs": "jsr:@std/fs@^1.0.18"
  }
}
