{
  "importMap": "import_map.json",
  "name": "@nik-kita/brickyard",
  "version": "0.1.105",
  "exports": "./mod.ts",
  "compilerOptions": {
    "noUnusedLocals": false,
    "strictNullChecks": true,
    "strictBindCallApply": true
  },
  "exclude": [
    "dist",
    "node_modules",
    ".vite"
  ],
  "lint": {
    "rules": {
      "exclude": [
        "no-explicit-any",
        "ban-types",
        "no-empty",
        "no-unused-vars",
        "require-await"
      ]
    }
  }
}
